<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    #1
    <servlet>
        <servlet-name>myServlet</servlet-name>
        <servlet-class>com.moore.servlets.myServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>myServlet</servlet-name>
        <url-pattern>/myServlets</url-pattern>
    </servlet-mapping>

    #2
    <servlet>
        <servlet-name>menuOrder</servlet-name>
        <servlet-class>com.moore.servlets.menuOrder</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>menuOrder</servlet-name>
        <url-pattern>/SeafoodMansion</url-pattern>
    </servlet-mapping>

    #3
    <servlet>
        <servlet-name>myRestaurant</servlet-name>
        <servlet-class>com.moore.servlets.myRestaurant</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>myRestaurant</servlet-name>
        <url-pattern>/myRestaurant</url-pattern>
    </servlet-mapping>

</web-app>

